{
	"manifest_version": 1,
	"name": "Mi Extensión",
	"version": "1.0",
	"description": "Descripción de la extensión",
	"permissions": ["activeTab"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/icon16.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"options_ui": {
		"page": "options.html"
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"manifest_version": 2
}
